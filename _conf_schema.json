{
  "auto_enabled": {
    "description": "是否启用随机夺舍",
    "type": "bool",
    "default": true
  },
  "group_mode": {
    "description": "群名单模式",
    "type": "string",
    "options": ["whitelist", "blacklist", "none"],
    "default": "none"
  },
  "group_list": {
    "description": "名单中的群号列表",
    "type": "list",
    "hint": "示例: [123456, 234567]"
  },
  "auto_interval": {
    "description": "随机夺舍的随机间隔区间",
    "type": "object",
    "items": {
      "min_minutes": {
        "description": "最小分钟数",
        "type": "int",
        "default": 60
      },
      "max_minutes": {
        "description": "最大分钟数",
        "type": "int",
        "default": 480
      }
    }
  }
}

